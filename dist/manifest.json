{
  "manifest_version": 2,
  "name": "iSpy - a localStorage manager",
  "description": "This extension provides chrome users with a simple interface to localStorage",
  "version": "1.0.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}