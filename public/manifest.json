{
  "manifest_version": 2,
  "name": "iSpy",
  "description": "This extension provides chrome users with a simple interface to localStorage",
  "version": "2.3.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
